{
        "name": "gen1",
        "type": "AAE-classifier",
        "vectorizer": [
                        {
                                "type": "Linear", 
                                "in": 21,
                                "out": 8
                        },
                        {
                                "type": "Tanh"
                        }
                ],
        "encoder": [
                        {
                                "type": "Conv1d", 
                                "in": 8,
                                "out": 64,
                                "kernel": 8
                        },
                        {
                                "type": "Tanh"
                        },
                        {
                                "type": "Conv1d", 
                                "in": 64,
                                "out": 32,
                                "kernel": 6
                        },
                        {
                                "type": "Tanh"
                        },
                        {
                                "type": "Flatten"
                        },
                        {
                                "type": "Linear", 
                                "in": 256,
                                "out": 256
                        },
                        {
                                "type": "Tanh" 
                        },
                        {
                                "type": "Linear", 
                                "in": 256,
                                "out": 10
                        },
                        {
                                "type": "Tanh" 
                        }
                ],
        "decoder": [
                        {
                                "type": "Linear", 
                                "in": 10,
                                "out": 256
                        },
                        {
                                "type": "ReLU" 
                        },{
                                "type": "Linear", 
                                "in": 256,
                                "out": 256
                        },
                        {
                                "type": "ReLU" 
                        },
                        {
                                "type": "Unflatten", 
                                "in": 32,
                                "out": 8
                        },
                        {
                                "type": "ConvTranspose1d", 
                                "in": 32,
                                "out": 32,
                                "kernel": 6
                        },
