                                "type": "Linear", 
                                "in": 10,
                                "out": 128
                        },
                        {
                                "type": "ReLU" 
                        },
                        {
                                "type": "Linear", 
                                "in": 128,
                                "out": 2
                        },
                        {
                                "type": "LogSoftmax" 
                        }
                ],
        "devectorizer": [
                        {
                                "type": "Linear", 
                                "in": 8,
                                "out": 21
                        },
                        {
                                "type": "LogSoftmax" 
                        }
                ],
        "classifier": [
                        {
                                "type": "Linear", 
                                "in": 10,
                                "out": 128
                        },
                        {
                                "type": "ReLU" 
                        },
                        {
                                "type": "Linear", 
